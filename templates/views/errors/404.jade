extends ../../layouts/pc
block body
    .container
        h1!= '找不到 (404)'
        if 'object' == typeof errorMsg
            div(style='margin: 30px 0;').debug!= htmlStringify(errorMsg)
        else
            p.lead!= utils.textToHTML(errorMsg) || '如果你看到这个页面，你大概知道发生了什么吧～继续浏览好运^_^'
        p: a(href='/').btn.btn-primary.btn-lg 外滩主页
